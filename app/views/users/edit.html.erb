<h1>ユーザー編集画面</h1>

<%= form_with model: @user do |f| %>

  <h4>姓</h4>
  <%= f.text_field :last_name%>

  <h4>名</h4>
  <%= f.text_field :first_name%>

  <h4>姓カナ</h4>
  <%= f.text_field :last_name_kana%>

  <h4>名カナ</h4>
  <%= f.text_field :first_name_kana%>

  <h4>ニックネーム</h4>
  <%= f.text_field :nickname%>

  <h4>プロフィール画像</h4>
  <%= f.file_field :profile_image, accept: "image/*" %>


    <%= f.submit '更新', class: "btn btn-secondary" %>


<% end %>